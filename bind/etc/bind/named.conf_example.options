options {
  directory "/var/cache/bind";
  dnssec-validation auto;
  recursion yes;
  allow-recursion {
    internal;
  };
  allow-query {
    internal;
  };
  allow-query-cache {
    internal;
  };
  listen-on {
    EXAMPLE_BIND9IP_DOCKER;
  };
  forwarders {
    EXAMPLE_PIHOLEIP_DOCKER port 53;
  };
};